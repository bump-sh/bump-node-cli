{
  "name": "bump-cli",
  "description": "Bump CLI to interact with the API of developers.bump.sh",
  "version": "0.1.0",
  "author": "Paul Bonaud <paulr@bump.sh>",
  "bin": {
    "bump": "./bin/run"
  },
  "bugs": "https://github.com/bump-sh/bump-node-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^10.17.56",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "globby": "^10.0.2",
    "prettier": "^2.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://bump.sh",
  "keywords": [
    "api",
    "documentation",
    "openapi",
    "asyncapi",
    "bump",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bump",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bump-sh/bump-node-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "pretest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "fmt": "eslint . --ext .ts --config .eslintrc --fix",
    "version": "oclif-dev readme && git add README.md",
    "pack": "oclif-dev pack"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "cli-ux": "^5.5.1",
    "tslib": "^1.14.1",
    "yaml": "^1.10.2"
  }
}
